{
    "name": "vdhicts/conditions",
    "description": "Package for handling conditions",
    "authors": [
        {
            "name": "Dick van der Heiden",
            "email": "info@vdhicts.nl",
            "role": "developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/collections": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Vdhicts\\Conditions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vdhicts\\Conditions\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
